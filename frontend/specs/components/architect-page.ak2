                                              button-parts:[
                                                button-component{
                                                  parts:[
                                                    max-size-image(architect-page-edit-icon-img){
                                                      image:[
                                                        (architect-page-edit-icon);
                                                      ],
                                                    };
                                                  ],
                                                  mixins:[(action-button);(budget-button-background);(margin-l1);],
                                                  data-attrs:[],
                                                };
                                              ],
                                              modal-parts:(bill-data-modal),
                                            };
                                          ],
                                        };
                                      ],
                                      mixins:[],
                                    };
                                    td-element{
