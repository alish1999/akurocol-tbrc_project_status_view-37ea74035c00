                                            };
                                          ],
                                        };
                                      ],
                                      mixins:[],
                                    };
                                  ],
                                  mixins:[],
                                };
                                
                              ],
                            };      
                            conditional-by-flag-rendering{
                              field:(edit-mode-on),
                              parts:[
                                tr-element{
                                  parts:[
                                    
                                    td-element{
                                      colspan:(architect-last-row-colspan),
                                      parts:[
