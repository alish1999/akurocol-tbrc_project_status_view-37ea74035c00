                              mixins:[(borders-top-round2);],
                            };
                            field-as-list-renderer-component{
                              collection:(architect-page-data),
                              field:(architect-page-items),
                              impersonate:(architect-item-data),
                              parts:[
                                tr-element{
                                  parts:[
                                    td-element{
                                      parts:[
                                        styled-label-getter{
                                          label:(architect-item-vendor),
                                          mixins:[(bold-text);],
                                        };
                                      ],
                                      mixins:[(cell-style);(cell-left-style);(light-bg);],
                                    };
                                    td-element{
                                      parts:[
                                        label-getter{
                                          label:(architect-item-due-date),
                                        };
                                      ],
                                      mixins:[(cell-style);],
                                    };
                                    td-element{
                                      parts:[
                                        label-getter{
                                          label:(architect-item-code),
                                        };                           
                                      ],
                                      mixins:[(cell-style);(light-bg);],
                                    };
                                    td-element{
                                      parts:[
                                        label-getter{
                                          label:(architect-item-total),
                                        };                           
                                      ],
                                      mixins:[(cell-style);],
                                    };
                                    td-element{
                                      parts:[
                                        label-getter{
                                          label:(architect-item-paid),
                                        };                           
                                      ],
                                      mixins:[(cell-style);(light-bg);],
                                    };
                                    td-element{
                                      parts:[
                                        label-getter{
                                          label:(architect-item-pending),
                                        };                           
                                      ],
                                      mixins:[(cell-style);],
                                    };
                                    td-element{
