bootstrap-modal(architect-delete-modal){
  parts:[
    div-element{
      parts:[
        max-size-image(architect-delete-title-icon-img){
          image:(architect-delete-title-icon),
        };
        styled-label-getter{
          label:(architect-delete-title),
          mixins:[(bold-text);(font-size-large);],
        };
      ],
      mixins:[(display-flex);(align-items-center);(justify-content-center);(flex-gap1);],
    };
    styled-label-getter{
      label:(architect-delete-alert),
      mixins:[(text-align-center);(margin-y2);],
    };
    stable-form-container{
      form-action:(architect-delete-action),
      form-method:(architect-delete-method),
      parts:[
        (current-bill-id);
        submit-component(architect-delete-button){
          parts:[
            label-getter{
              label:(architect-confirm-label),
            };
          ],
          mixins:[
            (gray-background-link);(padding-x1);(padding-y05);(button-radius);
          ],
          data-attrs:[],
        };
        bootstrap-dismiss-modal-button{
          parts:[
            label-getter{
              label:(architect-cancel-label),
            };
          ],
          mixins:[
            (gray-background-link);(padding-x1);(padding-y05);(border-black);(button-radius);
          ],
        };
      ],
      mixins:[(display-flex);(align-items-center);(justify-content-center);(flex-gap1);],
    };
  ],
  open:[],
};

parametrized-representative-component(architect-page){
  params:[
    (project-url);
  ],
  collection:[
    (architect-page-data);
  ],
  parts:[
    template-extensor{
      template:[
        (general-template);
      ],
      parts:[
        div-element{
          parts:[
            bootstrap-container{
              parts:[
                bootstrap-row{
                  parts:[
                    bootstrap-col{
                      parts:[
                        h1-element{
                          parts:[
                            label-getter{
                              label:(architect-page-title),
                            };
                          ],
                          mixins:[(font-size-xxxlarge);(font-family-one);],
                        };

                        field-as-collection-renderer-component{
                          collection:(architect-page-data),
                          field:(architect-page-files-link),
                          impersonate:(simple-link),
                          parts:[
                            simple-anchor{
                              parts:[],
                              link:(link-href),
                              label:(link-label),
                              target:(link-target),
                              mixins:[
                                (background-color-one-normal);(font-color-white);(dropdown-button-style);
                                (white-text-link);(button-radius);(padding-x1);
                              ],
                            };
                          ],
                        };
                      ],
                      mixins:[(display-flex);(flex-gap1);(align-items-center);(justify-content-between);],
                    };
                  ],
                  mixins:[(margin-b2);],
                };
                bootstrap-row{
                  parts:[
                    bootstrap-col{
                      parts:[
                        table-element(architect-page-table){
                          parts:[
                            tr-element{
                              parts:[
                                field-as-list-renderer-component{
                                  collection:(architect-page-data),
                                  field:(architect-page-columns),
