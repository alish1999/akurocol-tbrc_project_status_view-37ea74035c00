                                                    };
                                                  ],
                                                  mixins:[(button-color-four);(button-boder-styles);(button-radius);],
                                                  data-attrs:[],
                                                };
                                              ],
                                              modal-parts:(new-bill-data-modal),
                                            };
                                          ],
                                          mixins:[(display-flex);(justify-content-between);(align-items-center);(padding-y1);],
                                        };
                                      ],
                                      mixins:[(cell-style);],
                                    };
                                    td-element{ //vacios
                                      parts:[],
                                      mixins:[],
                                    };
                                    td-element{ //vacios
                                      parts:[],
                                      mixins:[],
                                    };
                                  ],
                                  mixins:[],
                                };  
                              ],
                            };                      
                          ],
                          mixins:[],
                        };
                      ],
                      mixins:[],
                    };
                  ],
                  mixins:[(margin-b1);],
                };
                bootstrap-row{
                  parts:[
                    bootstrap-col12{
                      parts:[
                        tbrc-paginator{
                          paginator-prev:(architect-page-paginator-prev),
                          paginator-next:(architect-page-paginator-next),
                          paginator-current:(architect-page-paginator-current),
                          paginator-total:(architect-page-paginator-total),
                        };
                      ],
                      mixins:[],
                    };
                  ],
                  mixins:[(margin-b4);],
                };
              ],
              mixins:[],
            };
          ],
          mixins:[(background-color-four-lightest);(padding-t4);(full-height);],
        };
        
        

      ]
    };
  ]
};